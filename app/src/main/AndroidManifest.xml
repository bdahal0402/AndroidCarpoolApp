<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bdtermproject">
    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@drawable/carpool"
        android:label="@string/app_name"
        android:roundIcon="@drawable/carpool"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name=".SignUp"/>
        <activity android:label="@string/app_name" android:name=".HomePage"/>
        <activity android:label="@string/app_name" android:name=".UserSettings"/>



    </application>

</manifest>